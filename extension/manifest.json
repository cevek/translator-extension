{
  "name": "Page Redder",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["mystyles.css"],
      "js": ["dict.js", "myscript.js"],
      "run_at": "document_end"

    }
  ],
  "manifest_version": 2
}